        "    csv_path='battery_cost_results.csv'\n",
        "):\n",
        "    # 1) Preprocess timestamps and usage\n",
        "    df = df.copy()\n",
        "    df['Timestamp'] = pd.to_datetime(df['Timestamp'], utc=True).dt.tz_convert(None)\n",
        "    df['month'] = df['Timestamp'].dt.month\n",
        "    df['hour'] = df['Timestamp'].dt.hour\n",
        "\n",
        "    # 2) Vectorized rate assignment\n",
        "    summer_months = [6, 7, 8, 9]\n",
        "    super_off_peak_months = [3, 4, 5]\n",
        "    super_off_peak_hours = range(9, 14)\n",
        "    summer_peak = range(16, 21)\n",
        "    summer_part_peak = list(range(14, 16)) + list(range(21, 23))\n",
        "    winter_peak = range(16, 21)\n",
        "\n",
        "    # Default rates: summer non-peak vs non-summer non-peak\n",
        "    df['rate'] = np.where(df['month'].isin(summer_months), 0.12220, 0.12189)\n",
        "\n",
        "    # Override specific periods\n",
        "    df.loc[df['month'].isin(summer_months) & df['hour'].isin(summer_peak), 'rate'] = 0.20832\n",
        "    df.loc[df['month'].isin(summer_months) & df['hour'].isin(summer_part_peak), 'rate'] = 0.16020\n",
        "    df.loc[df['month'].isin(super_off_peak_months) & df['hour'].isin(super_off_peak_hours), 'rate'] = 0.04451\n",
        "    df.loc[~df['month'].isin(summer_months) & df['hour'].isin(winter_peak), 'rate'] = 0.17965\n",
        "\n",
        "    # 3) Prepare vectors\n",
        "    U = df['Usage_kWh'].values\n",
        "    R = df['rate'].values\n",
        "    T = len(U)\n",
        "\n",
        "    # Degradation mask (1 at first period of each day)\n",
        "    df['date'] = df['Timestamp'].dt.date\n",
        "    degradation_mask = (df['date'] != df['date'].shift()).astype(float).values\n",
        "\n",
        "    # Grid-only cost\n",
        "    grid_only_cost = (U * R).sum()\n",
        "\n",
        "    # 4) Build one DPP CVXPY problem\n",
        "    E_param = cp.Parameter(nonneg=True)\n",
        "    B = cp.Variable(T)\n",
        "    C = cp.Variable(T)\n",
        "    D = cp.Variable(T)\n",
        "\n",
        "    constraints = [B[0] == 0]\n",
        "    for t in range(T):\n",
        "        constraints += [\n",
        "            B[t] >= 0,\n",
        "            B[t] <= E_param,\n",
        "            D[t] >= 0,\n",
        "            D[t] <= P * B[t],\n",
        "            C[t] >= 0,\n",
        "            C[t] <= E_param\n",
        "        ]\n",
        "        if t > 0:\n",
        "            constraints += [B[t] == B[t-1] - D[t-1]/P + C[t-1]]\n",
        "\n",
        "    G = U - D + C/P\n",
        "    constraints += [G >= 0]\n",
        "\n",
        "    base_cost = cp.sum(cp.multiply(G, R)) + Bd * cp.sum(cp.multiply(degradation_mask * B, R))\n",
        "    problem = cp.Problem(cp.Minimize(base_cost), constraints)\n",
        "\n",
        "    # 5) Sweep E values, reusing the problem\n",
        "    install_costs = {\n",
        "        e: Ci_per_kWh * (e ** 0.75)\n",
        "        for e in range(start_E, max_E + 1, step_size)\n",
        "    }\n",
        "    results = []\n",
        "\n",
        "    for e, ic in install_costs.items():\n",
        "        E_param.value = e\n",
        "        problem.solve(warm_start=True, verbose=False)  # default solver\n",
        "        if problem.status == cp.OPTIMAL:\n",
        "            total_cost = base_cost.value + ic\n",
        "        else:\n",
        "            total_cost = np.inf\n",
        "\n",
        "        results.append((e, total_cost))\n",
        "        pd.DataFrame(results, columns=[\"Battery_kWh\", \"Total_Cost\"]).to_csv(csv_path, index=False)\n",
        "        print(f\"E = {e} kWh ‚Üí Total Cost = ${total_cost:.2f}\")\n",
        "\n",
        "    # 6) Select best\n",
        "    feasible = [(e, c) for e, c in results if np.isfinite(c)]\n",
        "    if feasible:\n",
        "        best_E, best_cost = min(feasible, key=lambda x: x[1])\n",
        "        net_savings = grid_only_cost - best_cost\n",
        "        print(f\"\\nBest Battery Size: {best_E} kWh\")\n",
        "        print(f\"Battery Total Cost: ${best_cost:.2f}\")\n",
        "        print(f\"Grid-Only Cost: ${grid_only_cost:.2f}\")\n",
        "        print(f\"Net Savings: ${net_savings:.2f}\")\n",
        "        return best_E, best_cost, grid_only_cost, pd.DataFrame(results, columns=[\"Battery_kWh\", \"Total_Cost\"])\n",
        "    else:\n",
        "        print(\"No feasible solution found within the tested range.\")\n",
        "        return None, None, grid_only_cost, pd.DataFrame(results, columns=[\"Battery_kWh\", \"Total_Cost\"])\n"
